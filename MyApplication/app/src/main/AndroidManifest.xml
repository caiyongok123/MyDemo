<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:uses-featureandroid="http://schemas.android.com/apk/res-auto"
    package="com.example.cy.myapplication">

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <uses-feature android:name="android.software.live_wallpaper" />

    <uses-permission
        android:name="android.permission.BIND_WALLPAPER"
        tools:ignore="ProtectedPermissions" />
    <uses-permission
        android:name="android.permission.DEVICE_POWER"
        tools:ignore="ProtectedPermissions" />

    <application
        android:name=".activity.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/tx"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TabLayoutTestActivity" />
        <activity android:name=".activity.CoordinatorLayoutTestActivity" />
        <activity android:name=".activity.GifActivity" />
        <activity android:name=".activity.FragmentAnimActivity" />
        <activity android:name=".activity.AesActivity" />
        <activity android:name=".activity.ThreadActivity" />
        <activity android:name=".activity.ImagesActivity" />
        <activity android:name=".activity.SystemDownLoadActivity" />
        <activity android:name=".activity.SensorActivity" />
        <activity android:name=".activity.CalendarActivity" />
        <activity android:name=".activity.NetImagesActivity" />
        <activity
            android:name="com.leon.vuforia.ImageTargets"
            android:configChanges="orientation|keyboardHidden|screenSize|smallestScreenSize"
            android:theme="@style/SampleAppsTheme" />
        <activity android:name=".activity.GreenDaoActivity" />
        <activity android:name=".activity.MvvmActivity" />
        <activity android:name=".activity.VitamioBundleActivity" />
        <activity android:name=".activity.JiecaoActivity" />
        <activity
            android:name="io.vov.vitamio.activity.InitActivity"
            android:configChanges="orientation|screenSize|smallestScreenSize|keyboard|keyboardHidden|navigation"
            android:launchMode="singleTop"
            android:theme="@android:style/Theme.NoTitleBar"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".activity.AnimalUpAndDownActivity" />
        <activity android:name=".activity.mpandroidchart.MPAndroidChartActivity" />
        <activity android:name=".activity.mpandroidchart.ZheXianActivity" />
        <activity android:name=".activity.DialogFragmentActivity" />
        <activity android:name=".activity.BarActivity" />
        <activity android:name=".activity.CameraPreviewActivity" />
        <activity android:name=".activity.CustomLoadingActivity" />
        <activity android:name=".activity.qrscan.QrScanActivity" />
        <activity android:name=".activity.RsaActivity" />

        <service
            android:name=".activity.wallpapers.Mp4LiveWallpaperService"
            android:enabled="true"
            android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService" />
            </intent-filter>

            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/livepaper" />
        </service>
        <service
            android:name=".activity.wallpapers.CameraLiveWallpaperService"
            android:enabled="true"
            android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService" />
            </intent-filter>

            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/livepaper" />
        </service>

        <activity android:name=".activity.wallpapers.LiveWallpapersActivity" />
        <activity android:name=".activity.filepath.FilePathActivity" />
        <activity android:name=".activity.DrawerLayoutActivity" />
        <activity android:name=".activity.sound.SoundCompoundActivity"></activity>
    </application>

</manifest>